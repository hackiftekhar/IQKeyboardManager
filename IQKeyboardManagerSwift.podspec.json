{
    "name": "IQKeyboardManagerSwift",
    "version": "7.1.0",
    "source": {
        "git": "https://github.com/hackiftekhar/IQKeyboardManager.git",
        "tag": "7.1.0"
    },
    "summary": "Codeless drop-in universal library allows to prevent issues of keyboard sliding up and cover UITextField/UITextView.",
    "homepage": "https://github.com/hackiftekhar/IQKeyboardManager",
    "screenshots": "https://raw.githubusercontent.com/hackiftekhar/IQKeyboardManager/master/Screenshot/IQKeyboardManagerScreenshot.png",
    "license": "MIT",
    "authors": {
        "Iftekhar Qurashi": "hack.iftekhar@gmail.com"
    },
    "platforms": {
        "ios": "13.0"
    },
    "swift_versions": [
        "5.6",
        "5.7",
        "5.8",
        "5.9"
    ],
    "source_files": [
        "IQKeyboardManagerSwift/IQKeyboardManager/**/*.{swift}"
    ],
    "dependencies": {
        "IQKeyboardNotification": [],
        "IQTextInputViewNotification": [],
        "IQKeyboardCore": []
    },
    "resource_bundles": {
        "IQKeyboardManagerSwift": "IQKeyboardManagerSwift/PrivacyInfo.xcprivacy"
    },
    "subspecs": [
        {
            "name": "Resign",
            "source_files": [
                "IQKeyboardManagerSwift/IQKeyboardResign/**/*.{swift}"
            ],
            "platforms":{
                "ios":"13.0"
            },
            "frameworks": [
                "UIKit"
            ]
        },
        {
            "name": "Appearance",
            "source_files": [
                "IQKeyboardManagerSwift/IQKeyboardAppearance/**/*.{swift}"
            ],
            "platforms":{
                "ios":"13.0"
            },
            "frameworks": [
                "UIKit"
            ]
        }
    ],
    "default_subspecs": ["none"],
    "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics",
        "QuartzCore"
    ],
    "libraries": "swiftCoreGraphics",
    "xcconfig": {
        "LIBRARY_SEARCH_PATHS": "$(SDKROOT)/usr/lib/swift"
    },
    "requires_arc": true
}
